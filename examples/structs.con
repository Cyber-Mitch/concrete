mod Structs {
    struct Node {
        a: i32,
        b: i32,
    }

    fn main() -> i32 {
        let mut x: Node = create_node(2, 4);
        x.a = 100;
        modify_node(&mut x);
        return x.a + x.b;
    }

    fn create_node(a: i32, b: i32) -> Node {
        let x: Node = Node {
            a: a,
            b: b,
        };
        return x;
    }

    fn modify_node(x: &mut Node) {
        x.a = 1;
    }
}
